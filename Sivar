-----------------------
-- í”Œë¼ì´ ê¸°ëŠ¥
-----------------------
local function setupFly()
    local player = game.Players.LocalPlayer
    local char = player.Character
    if not char then return end
    
    local hrp = char:FindFirstChild("HumanoidRootPart")
    local hum = char:FindFirstChildOfClass("Humanoid")
    if not hrp or not hum then return end
    
    if flyEnabled then
        hum.PlatformStand = true
        
        if flyBV then flyBV:Destroy() end
        
        flyBV = Instance.new("BodyVelocity")
        flyBV.MaxForce = Vector3.new(1, 1, 1) * 1e9
        flyBV.Velocity = Vector3.new(0, 0, 0)
        flyBV.Parent = hrp
        
        if flyConnection then flyConnection:Disconnect() end
        
        flyConnection = game:GetService("RunService").Heartbeat:Connect(function()
            if not (flyEnabled and hrp and hrp.Parent) then return end
            
            local move = Vector3.new(0, 0, 0)
            local cam = workspace.CurrentCamera
            
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.W) then
                move = move + cam.CFrame.LookVector
            end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.S) then
                move = move - cam.CFrame.LookVector
            end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.A) then
                move = move - cam.CFrame.RightVector
            end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.D) then
                move = move + cam.CFrame.RightVector
            end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.Space) then
                move = move + Vector3.new(0, 1, 0)
            end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftControl) then
                move = move + Vector3.new(0, -1, 0)
            end
            
            if move.Magnitude > 0 then
                move = move.Unit * flySpeed
            end
            
            flyBV.Velocity = move
        end)
    else
        if flyBV then flyBV:Destroy() flyBV = nil end
        if flyConnection then flyConnection:Disconnect() flyConnection = nil end
        if hum then hum.PlatformStand = false end
    end
end

-----------------------
-- ì†ë„ ê¸°ëŠ¥ (ì™„ì „ ì¬ì‘ì„±)
-----------------------
local function setupSpeed()
    local player = game.Players.LocalPlayer
    local char = player.Character
    if not char then return end
    
    local hum = char:FindFirstChildOfClass("Humanoid")
    if not hum then return end
    
    if speedEnabled then
        -- í˜„ì¬ ì†ë„ë¥¼ ì €ì¥í•˜ê³  ìƒˆ ì†ë„ ì ìš©
        originalSpeed = 16
        hum.WalkSpeed = speedValue
        
        -- ê¸°ì¡´ ì—°ê²° ì œê±°
        if speedConnection then
            speedConnection:Disconnect()
        end
        
        -- ì§€ì†ì ìœ¼ë¡œ ì†ë„ ê°ì‹œ ë° ìœ ì§€
        speedConnection = game:GetService("RunService").RenderStepped:Connect(function()
            if speedEnabled and hum and hum.Parent then
                if hum.WalkSpeed ~= speedValue then
                    hum.WalkSpeed = speedValue
                end
            end
        end)
        
        print("[ì†ë„] í™œì„±í™” - ê°’:", speedValue)
    else
        -- ì†ë„ ë¹„í™œì„±í™”
        if speedConnection then
            speedConnection:Disconnect()
            speedConnection = nil
        end
        
        if hum and hum.Parent then
            hum.WalkSpeed = 16
        end
        
        print("[ì†ë„] ë¹„í™œì„±í™”")
    end
end

-----------------------
-- ì í”„ ê¸°ëŠ¥ (ì™„ì „ ì¬ì‘ì„±)
-----------------------
local function setupJump()
    local player = game.Players.LocalPlayer
    local char = player.Character
    if not char then return end
    
    local hum = char:FindFirstChildOfClass("Humanoid")
    if not hum then return end
    
    if jumpEnabled then
        -- í˜„ì¬ ì í”„ë ¥ ì €ì¥í•˜ê³  ìƒˆ ì í”„ë ¥ ì ìš©
        originalJump = 50
        hum.JumpPower = jumpValue
        hum.JumpHeight = jumpValue / 10
        
        -- ê¸°ì¡´ ì—°ê²° ì œê±°
        if jumpConnection then
            jumpConnection:Disconnect()
        end
        
        -- ì§€ì†ì ìœ¼ë¡œ ì í”„ë ¥ ê°ì‹œ ë° ìœ ì§€
        jumpConnection = game:GetService("RunService").RenderStepped:Connect(function()
            if jumpEnabled and hum and hum.Parent then
                if hum.JumpPower ~= jumpValue then
                    hum.JumpPower = jumpValue
                    hum.JumpHeight = jumpValue / 10
                end
            end
        end)
        
        print("[ì í”„] í™œì„±í™” - ê°’:", jumpValue)
    else
        -- ì í”„ë ¥ ë¹„í™œì„±í™”
        if jumpConnection then
            jumpConnection:Disconnect()
            jumpConnection = nil
        end
        
        if hum and hum.Parent then
            hum.JumpPower = 50
            hum.JumpHeight = 5
        end
        
        print("[ì í”„] ë¹„í™œì„±í™”")
    end
end

-----------------------
-- TPì›Œí¬ ê¸°ëŠ¥ (ì™„ì „ ì¬ì‘ì„±)
-----------------------
local function setupTPWalk()
    local player = game.Players.LocalPlayer
    local char = player.Character
    if not char then return end
    
    local hrp = char:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    
    if tpWalkEnabled then
        tpWalkLastPos = hrp.Position
        
        if tpWalkConnection then
            tpWalkConnection:Disconnect()
        end
        
        tpWalkConnection = game:GetService("RunService").Heartbeat:Connect(function()
            if not (tpWalkEnabled and hrp and hrp.Parent) then return end
            
            local currentPos = hrp.Position
            local distance = (currentPos - tpWalkLastPos).Magnitude
            
            -- ì„¤ì •ëœ ì†ë„ë³´ë‹¤ ë§ì´ ì´ë™í–ˆì„ ë•Œ í…”ë ˆí¬íŠ¸
            if distance > tpWalkValue then
                local direction = (currentPos - tpWalkLastPos).Unit
                hrp.CFrame = CFrame.new(tpWalkLastPos + direction * tpWalkValue)
            end
            
            tpWalkLastPos = hrp.Position
        end)
        
        print("[TPì›Œí¬] í™œì„±í™” - ê°’:", tpWalkValue)
    else
        if tpWalkConnection then
            tpWalkConnection:Disconnect()
            tpWalkConnection = nil
        end
        tpWalkLastPos = nil
        print("[TPì›Œí¬] ë¹„í™œì„±í™”")
    end
end

-----------------------
-- í…”ë ˆí¬íŠ¸ í•¨ìˆ˜
-----------------------
local function teleportTo(position)
    local player = game.Players.LocalPlayer
    local char = player.Character
    if char then
        local hrp = char:FindFirstChild("HumanoidRootPart")
        if hrp then
            hrp.CFrame = CFrame.new(position)
            return true
        end
    end
    return false
end

-----------------------
-- í”Œë ˆì´ì–´ ëª©ë¡ ì—…ë°ì´íŠ¸
-----------------------
local function getPlayerList()
    local list = {}
    for _, p in pairs(game.Players:GetPlayers()) do
        if p ~= game.Players.LocalPlayer then
            table.insert(list, p.Name)
        end
    end
    return list
end

