-- ë©”ì¸ íƒ­
local S = MT:CreateSection("ì´ë™ ê¸°ëŠ¥")
local Spd, JP, Grv, FlySpd, FlyE = 16, 50, 196.2, 50, false
local FlyBV, FlyBG, FlyC, NC, IJ, JmpC, NocC

MT:CreateSlider({Name="ì†ë„ 1~5000",Range={1,5000},Increment=1,CurrentValue=16,Flag="S",Callback=function(v)Spd=v end})
MT:CreateLabel("í˜„ì¬ ì†ë„: 16")
RS.Heartbeat:Connect(function()if P.Character then local H=P.Character:FindFirstChildOfClass("Humanoid")if H then H.WalkSpeed=Spd end end end)

MT:CreateSlider({Name="ì í”„ë ¥ 1~5000",Range={1,5000},Increment=1,CurrentValue=50,Flag="J",Callback=function(v)JP=v end})
MT:CreateLabel("í˜„ì¬ ì í”„ë ¥: 50")
RS.Heartbeat:Connect(function()if P.Character then local H=P.Character:FindFirstChildOfClass("Humanoid")if H then H.JumpPower=JP end end end)

MT:CreateSlider({Name="ì¤‘ë ¥ 1~5000",Range={1,5000},Increment=1,CurrentValue=196.2,Flag="G",Callback=function(v)workspace.Gravity=v end})
MT:CreateLabel("í˜„ì¬ ì¤‘ë ¥: 196.2")
RS.Heartbeat:Connect(function()MT:CreateLabel("í˜„ì¬ ì¤‘ë ¥: "..math.floor(workspace.Gravity))end)

MT:CreateSection("í”Œë¼ì´")
MT:CreateSlider({Name="í”Œë¼ì´ ì†ë„ 1~5000",Range={1,5000},Increment=1,CurrentValue=50,Flag="FS",Callback=function(v)FlySpd=v if FlyBV then FlyBV.MaxForce=Vector3.new(v*1000,v*1000,v*1000)end end})
MT:CreateToggle({Name="í”Œë¼ì´ ON/OFF",CurrentValue=false,Flag="F",Callback=function(s)FlyE=s
if FlyE then local C=P.Character or WFC()local H=C:FindFirstChildOfClass("Humanoid")local R=C:FindFirstChild("HumanoidRootPart")if R and H then
if FlyBV then FlyBV:Destroy()end if FlyBG then FlyBG:Destroy()end
FlyBV=Instance.new("BodyVelocity")FlyBV.Velocity=Vector3.new()FlyBV.MaxForce=Vector3.new(FlySpd*1000,FlySpd*1000,FlySpd*1000)FlyBV.P=10000 FlyBV.Parent=R
FlyBG=Instance.new("BodyGyro")FlyBG.MaxTorque=Vector3.new(4e4,4e4,4e4)FlyBG.P=10000 FlyBG.CFrame=R.CFrame FlyBG.Parent=R
H.PlatformStand=true
FlyC=RS.Heartbeat:Connect(function()if FlyE and P.Character and R and FlyBV then
local D=Vector3.new()local C=workspace.CurrentCamera
if UI:IsKeyDown(Enum.KeyCode.W)then D=D+C.CFrame.LookVector end
if UI:IsKeyDown(Enum.KeyCode.S)then D=D-C.CFrame.LookVector end
if UI:IsKeyDown(Enum.KeyCode.A)then D=D-C.CFrame.RightVector end
if UI:IsKeyDown(Enum.KeyCode.D)then D=D+C.CFrame.RightVector end
if UI:IsKeyDown(Enum.KeyCode.Space)then D=D+Vector3.new(0,1,0)end
if UI:IsKeyDown(Enum.KeyCode.LeftControl)then D=D+Vector3.new(0,-1,0)end
FlyBV.Velocity=D.Magnitude>0 and D.Unit*FlySpd or D
FlyBG.CFrame=C.CFrame end end)
R:Notify({Title="í”Œë¼ì´",Content="WASD+Space+Ctrl",Duration=3})else R:Notify({Title="ì˜¤ë¥˜",Content="ìºë¦­í„° ì—†ìŒ",Duration=2})end
else if FlyC then FlyC:Disconnect()FlyC=nil end
if FlyBV then FlyBV:Destroy()FlyBV=nil end
if FlyBG then FlyBG:Destroy()FlyBG=nil end
if P.Character then local H=P.Character:FindFirstChildOfClass("Humanoid")if H then H.PlatformStand=false end end
R:Notify({Title="í”Œë¼ì´",Content="OFF",Duration=2})end end})

MT:CreateToggle({Name="ë¬´í•œ ì í”„",CurrentValue=false,Flag="IJ",Callback=function(s)IJ=s
if IJ then JmpC=U.JumpRequest:Connect(function()if P.Character then local H=P.Character:FindFirstChildOfClass("Humanoid")if H then H:ChangeState(Enum.HumanoidStateType.Jumping)end end end)
else if JmpC then JmpC:Disconnect()JmpC=nil end end end})

MT:CreateToggle({Name="ë²½ëš«ê¸°",CurrentValue=false,Flag="NC",Callback=function(s)NC=s
if NC then NocC=RS.Stepped:Connect(function()if P.Character then for _,v in pairs(P.Character:GetDescendants())do if v:IsA("BasePart")then v.CanCollide=false end end end)end
if not NC and NocC then NocC:Disconnect()NocC=nil end end})

-- PCLD
local PCLD, PCLDM, PCLDC = false, {}, nil
local function CIM(p)if not p.Character or not p.Character:FindFirstChild("HumanoidRootPart")then return nil end
local m=Instance.new("Part")m.Size=Vector3.new(1,1,1)m.Transparency=1 m.Anchored=true m.CanCollide=false m.Parent=workspace
local b=Instance.new("BillboardGui")b.Size=UDim2.new(0,150,0,50)b.StudsOffset=Vector3.new(0,3,0)b.AlwaysOnTop=true b.Parent=m
local f=Instance.new("Frame")f.Size=UDim2.new(1,0,1,0)f.BackgroundColor3=Color3.fromRGB(0,0,0)f.BackgroundTransparency=0.3 f.BorderSizePixel=2 f.BorderColor3=Color3.fromRGB(255,100,100)f.Parent=b
local i=Instance.new("TextLabel")i.Size=UDim2.new(1,0,0.5,0)i.Position=UDim2.new(0,0,0,0)i.BackgroundTransparency=1 i.Text="ID: "..p.UserId i.TextColor3=Color3.fromRGB(255,200,200)i.TextStrokeTransparency=0.3 i.TextStrokeColor3=Color3.fromRGB(255,0,0)i.Font=Enum.Font.GothamBold i.TextSize=18 i.Parent=f
local n=Instance.new("TextLabel")n.Size=UDim2.new(1,0,0.5,0)n.Position=UDim2.new(0,0,0.5,0)n.BackgroundTransparency=1 n.Text=p.Name n.TextColor3=Color3.fromRGB(255,255,255)n.TextStrokeTransparency=0 n.TextStrokeColor3=Color3.fromRGB(0,0,0)n.Font=Enum.Font.Gotham n.TextSize=14 n.Parent=f
return m end

MT:CreateSection("PCLD - ID í‘œì‹œ")
MT:CreateToggle({Name="PCLD ON/OFF",CurrentValue=false,Flag="P",Callback=function(s)PCLD=s
if PCLD then for _,v in pairs(PCLDM)do pcall(function()v:Destroy()end)end PCLDM={}
for _,p in ipairs(PL:GetPlayers())do if p~=P then local m=pcall(function()return CIM(p)end)if m and type(m)=="userdata"then PCLDM[p]=m end end end
PCLDC=RS.Heartbeat:Connect(function()if PCLD then for p,m in pairs(PCLDM)do pcall(function()if p and p.Character and p.Character:FindFirstChild("HumanoidRootPart")then m.CFrame=p.Character.HumanoidRootPart.CFrame else m:Destroy()PCLDM[p]=nil end end)end
for _,p in ipairs(PL:GetPlayers())do if p~=P and not PCLDM[p]then local m=pcall(function()return CIM(p)end)if m and type(m)=="userdata"then PCLDM[p]=m end end end end end)
R:Notify({Title="PCLD",Content="ON",Duration=2})else if PCLDC then PCLDC:Disconnect()PCLDC=nil end
for _,m in pairs(PCLDM)do pcall(function()m:Destroy()end)end PCLDM={} R:Notify({Title="PCLD",Content="OFF",Duration=2})end end})

-- í…”ë ˆí¬íŠ¸ íƒ­
local SelP, TPing = nil, false
local PD = TT:CreateDropdown({Name="í”Œë ˆì´ì–´ ì„ íƒ",Options=(function()local o={}for _,p in ipairs(PL:GetPlayers())do if p~=P then table.insert(o,p.Name)end end return o end)(),CurrentOption={"ì„ íƒ"},MultipleOptions=false,Flag="PD",Callback=function(s)if s and #s>0 then for _,p in ipairs(PL:GetPlayers())do if p.Name==s[1]then SelP=p R:Notify({Title="ì„ íƒë¨",Content=p.Name,Duration=2})break end end end end})
local SL = TT:CreateLabel("ìƒíƒœ: ëŒ€ê¸°")
TT:CreateButton({Name="ğŸ¯ í…”ë ˆí¬íŠ¸ (1ì´ˆ)",Callback=function()
if TPing then R:Notify({Title="ê²½ê³ ",Content="ì´ë¯¸ ì§„í–‰ì¤‘",Duration=2})return end
if not SelP then R:Notify({Title="ì˜¤ë¥˜",Content="í”Œë ˆì´ì–´ ì„ íƒ í•„ìš”",Duration=2})return end
local ok=false for _,p in ipairs(PL:GetPlayers())do if p==SelP then ok=true break end end
if not ok then R:Notify({Title="ì˜¤ë¥˜",Content="í”Œë ˆì´ì–´ ì—†ìŒ",Duration=2})SelP=nil return end
if not SelP.Character or not SelP.Character:FindFirstChild("HumanoidRootPart")then R:Notify({Title="ì˜¤ë¥˜",Content="ëŒ€ìƒ ìºë¦­í„° ì—†ìŒ",Duration=2})return end
if not P.Character or not P.Character:FindFirstChild("HumanoidRootPart")then R:Notify({Title="ì˜¤ë¥˜",Content="ë‚´ ìºë¦­í„° ì—†ìŒ",Duration=2})return end
TPing=true SL:Set("ìƒíƒœ: ì¤€ë¹„ì¤‘")
for i=1,10 do SL:Set("ìƒíƒœ: "..string.format("%.1f",(11-i)/10).."ì´ˆ")pcall(function()if P.Character and P.Character:FindFirstChild("HumanoidRootPart")then P.Character.HumanoidRootPart.CFrame=P.Character.HumanoidRootPart.CFrame+Vector3.new(math.random(-20,20)/10,math.random(-20,20)/10,math.random(-20,20)/10)end end)wait(0.1)end
pcall(function()local C=SelP.Character.HumanoidRootPart.CFrame P.Character.HumanoidRootPart.CFrame=C+Vector3.new(0,5,0)
local E=Instance.new("Part")E.Size=Vector3.new(3,3,3)E.Position=P.Character.HumanoidRootPart.Position E.BrickColor=BrickColor.new("Bright blue")E.Material=Enum.Material.Neon E.Transparency=0.3 E.Anchored=true E.CanCollide=false E.Parent=workspace
for i=1,10 do E.Transparency=E.Transparency+0.07 E.Size=E.Size+Vector3.new(0.3,0.3,0.3)wait(0.03)end E:Destroy()end)
R:Notify({Title="ì™„ë£Œ",Content=SelP.Name.." ì´ë™",Duration=3})SL:Set("ìƒíƒœ: ëŒ€ê¸°")TPing=false end})
UI.InputBegan:Connect(function(i,g)if not g and i.KeyCode==Enum.KeyCode.T then SelP and TT:CreateButton({Name="ğŸ¯ í…”ë ˆí¬íŠ¸ (1ì´ˆ)",Callback=function()end})or R:Notify({Title="ì•Œë¦¼",Content="ì„ íƒ í•„ìš”",Duration=2})end end)
TT:CreateButton({Name="ğŸ”„ ìƒˆë¡œê³ ì¹¨",Callback=function()local o={}for _,p in ipairs(PL:GetPlayers())do if p~=P then table.insert(o,p.Name)end end PD:Set(o)if SelP then local e=false for _,p in ipairs(PL:GetPlayers())do if p==SelP then e=true break end end if not e then SelP=nil end end R:Notify({Title="ê°±ì‹ ",Content=#o.."ëª…",Duration=2})end})
local SIL = TT:CreateLabel("ì„ íƒ: ì—†ìŒ")
RS.Heartbeat:Connect(function()if SelP then local t=SelP.Name pcall(function()if SelP.Character and SelP.Character:FindFirstChild("HumanoidRootPart")and P.Character and P.Character:FindFirstChild("HumanoidRootPart")then local d=(P.Character.HumanoidRootPart.Position-SelP.Character.HumanoidRootPart.Position).Magnitude SIL:Set("ì„ íƒ: "..t..string.format(" (%.1f)",d))else SIL:Set("ì„ íƒ: "..t.." (ìºë¦­í„° ì—†ìŒ)")end end)else SIL:Set("ì„ íƒ: ì—†ìŒ")end end)

