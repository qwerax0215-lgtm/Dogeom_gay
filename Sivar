-- ìºë¦­í„° ë³€ê²½
game.Players.LocalPlayer.CharacterAdded:Connect(function(c)wait(1)
    local h=c:WaitForChild("Humanoid",5);local r=c:WaitForChild("HumanoidRootPart",5)
    if not h or not r then return end
    if fEnabled then wait(0.2)setupFly()end
    if sEnabled then wait(0.2)h.WalkSpeed=sValue;setupSpeed()end
    if tEnabled then wait(0.2)setupTP()end
    if jEnabled then wait(0.2)h.JumpPower=jValue;h.JumpHeight=jValue/10;setupJump()end
end)

Rayfield:Notify({Title="Dong Universe",Content="ë¡œë“œ ì™„ë£Œ! (v3.0)",Duration=2})
