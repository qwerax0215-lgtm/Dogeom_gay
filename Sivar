local W = R:CreateWindow({Name="ìœ ë‹ˆë²„ìŠ¤ (Universe)",LoadingTitle="ë¡œë”©ì¤‘",LoadingSubtitle="by Rayfield",ConfigurationSaving={Enabled=true,FolderName="UniverseHub",FileName="Config"},ToggleUIKeybind="K",KeySystem=false})
local P, U, RS, PL, T, UI = game:Playeâ€Šrâ€Šs.LocalPlayer, game:GetService("UserInputService"), game:GetService("RunService"), game:GetService("Players"), game:GetService("TweenService"), game:GetService("UserInputService")
local function WFC() repeat wait() until P.Character and P.Character:FindFirstChild("HumanoidRootPart") return P.Character end
local MT, TT, Misc = W:CreateTab("ë©”ì¸",4483362458), W:CreateTab("í…”ë ˆí¬íŠ¸",4483362458), W:CreateTab("ê·¸ ì™¸",4483362458)
