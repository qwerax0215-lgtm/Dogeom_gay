local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local Window = Rayfield:CreateWindow({Name="Dong Universe",LoadingTitle="Dong Universe",LoadingSubtitle="v3.0",ConfigurationSaving={Enabled=true,FolderName="DongUniverse",FileName="Config"},KeySystem=false})
local MainTab = Window:CreateTab("ë©”ì¸","home")
local TeleportTab = Window:CreateTab("í…”ë ˆí¬íŠ¸","arrow-right")
local OtherTab = Window:CreateTab("ê·¸ ì™¸","gamepad-2")
local InfoTab = Window:CreateTab("ì •ë³´","info")

-- ë³€ìˆ˜
local fEnabled,fSpeed,fBV,fConn = false,50
local sEnabled,sValue,sConn = false,50
local jEnabled,jValue,jConn = false,50
local tEnabled,tValue,tConn,tLast = false,50
local gEnabled,gValue = false,50
local aimEnabled = false
local selectedPlayer = nil

-- í•¨ìˆ˜
local function gP() local l={}for _,p in pairs(game.Players:GetPlayers())do if p~=game.Players.LocalPlayer then table.insert(l,p.Name)end end return l end
local function tp(p)local c=game.Players.LocalPlayer.Character;if c and c:FindFirstChild("HumanoidRootPart")then c.HumanoidRootPart.CFrame=CFrame.new(p)return true end return false end

-- í”Œë¼ì´
local function setupFly()
    local p=game.Players.LocalPlayer;local c=p.Character;if not c then return end
    local r=c:FindFirstChild("HumanoidRootPart");local h=c:FindFirstChildOfClass("Humanoid")
    if not r or not h then return end
    if fEnabled then
        h.PlatformStand=true;if fBV then fBV:Destroy()end
        fBV=Instance.new("BodyVelocity");fBV.MaxForce=Vector3.new(1,1,1)*1e9;fBV.Velocity=Vector3.new(0,0,0);fBV.Parent=r
        if fConn then fConn:Disconnect()end
        fConn=game:GetService("RunService").Heartbeat:Connect(function()
            if not(fEnabled and r and r.Parent)then return end
            local m=Vector3.new(0,0,0);local cam=workspace.CurrentCamera;local uis=game:GetService("UserInputService")
            if uis:IsKeyDown(Enum.KeyCode.W)then m=m+cam.CFrame.LookVector end
            if uis:IsKeyDown(Enum.KeyCode.S)then m=m-cam.CFrame.LookVector end
            if uis:IsKeyDown(Enum.KeyCode.A)then m=m-cam.CFrame.RightVector end
            if uis:IsKeyDown(Enum.KeyCode.D)then m=m+cam.CFrame.RightVector end
            if uis:IsKeyDown(Enum.KeyCode.Space)then m=m+Vector3.new(0,1,0)end
            if uis:IsKeyDown(Enum.KeyCode.LeftControl)then m=m+Vector3.new(0,-1,0)end
            if m.Magnitude>0 then m=m.Unit*fSpeed end;fBV.Velocity=m
        end)
    else
        if fBV then fBV:Destroy()fBV=nil end;if fConn then fConn:Disconnect()fConn=nil end;if h then h.PlatformStand=false end
    end
end

-- ì†ë„
local function setupSpeed()
    local p=game.Players.LocalPlayer;local c=p.Character;if not c then return end
    local h=c:FindFirstChildOfClass("Humanoid");if not h then return end
    if sEnabled then
        h.WalkSpeed=sValue;if sConn then sConn:Disconnect()end
        sConn=game:GetService("RunService").RenderStepped:Connect(function()
            if sEnabled and h and h.Parent and h.WalkSpeed~=sValue then h.WalkSpeed=sValue end
        end)
    else
        if sConn then sConn:Disconnect()sConn=nil end;if h and h.Parent then h.WalkSpeed=16 end
    end
end

-- ì í”„
local function setupJump()
    local p=game.Players.LocalPlayer;local c=p.Character;if not c then return end
    local h=c:FindFirstChildOfClass("Humanoid");if not h then return end
    if jEnabled then
        h.JumpPower=jValue;h.JumpHeight=jValue/10;if jConn then jConn:Disconnect()end
        jConn=game:GetService("RunService").RenderStepped:Connect(function()
            if jEnabled and h and h.Parent then
                if h.JumpPower~=jValue then h.JumpPower=jValue;h.JumpHeight=jValue/10 end
            end
        end)
    else
        if jConn then jConn:Disconnect()jConn=nil end;if h and h.Parent then h.JumpPower=50;h.JumpHeight=5 end
    end
end

-- TPì›Œí¬
local function setupTP()
    local p=game.Players.LocalPlayer;local c=p.Character;if not c then return end
    local r=c:FindFirstChild("HumanoidRootPart");if not r then return end
    if tEnabled then
        tLast=r.Position;if tConn then tConn:Disconnect()end
        tConn=game:GetService("RunService").Heartbeat:Connect(function()
            if not(tEnabled and r and r.Parent)then return end
            local cur=r.Position;local dist=(cur-tLast).Magnitude
            if dist>tValue then local dir=(cur-tLast).Unit;r.CFrame=CFrame.new(tLast+dir*tValue)end
            tLast=r.Position
        end)
    else if tConn then tConn:Disconnect()tConn=nil end;tLast=nil end
end

-- ì—ì„ TP
game:GetService("UserInputService").InputBegan:Connect(function(i)
    if i.KeyCode==Enum.KeyCode.Z and aimEnabled then
        local m=game.Players.LocalPlayer:GetMouse();if m and m.Hit then tp(m.Hit.p)
        Rayfield:Notify({Title="ì—ì„ TP",Content="ì´ë™í–ˆìŠµë‹ˆë‹¤",Duration=1})end
    end
end)

-- ë©”ì¸ íƒ­
MainTab:CreateSection("í”Œë¼ì´")
local fT=MainTab:CreateToggle({Name="í”Œë¼ì´",CurrentValue=false,Flag="FT",Callback=function(v)fEnabled=v;setupFly()end})
MainTab:CreateSlider({Name="í”Œë¼ì´ ì†ë„",Range={1,2000},Increment=1,CurrentValue=50,Flag="FS",Callback=function(v)fSpeed=v end})

MainTab:CreateSection("ì†ë„")
local sT=MainTab:CreateToggle({Name="ì†ë„",CurrentValue=false,Flag="ST",Callback=function(v)sEnabled=v;setupSpeed()Rayfield:Notify({Title="ì†ë„",Content=v and"í™œì„±í™”"or"ë¹„í™œì„±í™”",Duration=1})end})
MainTab:CreateSlider({Name="ì†ë„ ê°’",Range={1,2000},Increment=1,CurrentValue=50,Flag="SV",Callback=function(v)sValue=v;if sEnabled then local h=game.Players.LocalPlayer.Character;if h then h=h:FindFirstChildOfClass("Humanoid")if h then h.WalkSpeed=v end end end end})

MainTab:CreateSection("ì í”„")
local jT=MainTab:CreateToggle({Name="ì í”„",CurrentValue=false,Flag="JT",Callback=function(v)jEnabled=v;setupJump()Rayfield:Notify({Title="ì í”„",Content=v and"í™œì„±í™”"or"ë¹„í™œì„±í™”",Duration=1})end})
MainTab:CreateSlider({Name="ì í”„ë ¥",Range={1,500},Increment=1,CurrentValue=50,Flag="JV",Callback=function(v)jValue=v;if jEnabled then local h=game.Players.LocalPlayer.Character;if h then h=h:FindFirstChildOfClass("Humanoid")if h then h.JumpPower=v;h.JumpHeight=v/10 end end end end})

MainTab:CreateSection("TPì›Œí¬")
local tT=MainTab:CreateToggle({Name="TPì›Œí¬",CurrentValue=false,Flag="TT",Callback=function(v)tEnabled=v;setupTP()Rayfield:Notify({Title="TPì›Œí¬",Content=v and"í™œì„±í™”"or"ë¹„í™œì„±í™”",Duration=1})end})
MainTab:CreateSlider({Name="TPì›Œí¬ ì†ë„",Range={1,2000},Increment=1,CurrentValue=50,Flag="TV",Callback=function(v)tValue=v end})

MainTab:CreateSection("ì¤‘ë ¥")
local gT=MainTab:CreateToggle({Name="ì¤‘ë ¥",CurrentValue=false,Flag="GT",Callback=function(v)gEnabled=v;workspace.Gravity=v and gValue or 196.2 end})
MainTab:CreateSlider({Name="ì¤‘ë ¥ ê°’",Range={0,100},Increment=1,CurrentValue=50,Flag="GV",Callback=function(v)gValue=v;if gEnabled then workspace.Gravity=v end end})

-- í…”ë ˆí¬íŠ¸ íƒ­
TeleportTab:CreateSection("ì—ì„ TP")
local aT=TeleportTab:CreateToggle({Name="ì—ì„ TP (Z)",CurrentValue=false,Flag="AT",Callback=function(v)aimEnabled=v end})

TeleportTab:CreateSection("ìœ„ì¹˜ TP")
TeleportTab:CreateButton({Name="í•˜ëŠ˜ë¡œ(500)",Callback=function()local c=game.Players.LocalPlayer.Character;if c and c:FindFirstChild("HumanoidRootPart")then tp(c.HumanoidRootPart.Position+Vector3.new(0,500,0))end end})
TeleportTab:CreateButton({Name="ë•…ìœ¼ë¡œ",Callback=function()local c=game.Players.LocalPlayer.Character;if c and c:FindFirstChild("HumanoidRootPart")then
    local r=c.HumanoidRootPart;local h,pos=workspace:FindPartOnRay(Ray.new(r.Position+Vector3.new(0,10,0),Vector3.new(0,-1000,0)),c)
    if pos then tp(pos+Vector3.new(0,3,0))end
end end})
TeleportTab:CreateButton({Name="ìŠ¤í°ìœ¼ë¡œ",Callback=function()local s=workspace:FindFirstChild("SpawnLocation")or workspace:FindFirstChild("Spawn");tp(s and s.Position+Vector3.new(0,3,0)or Vector3.new(0,50,0))end})

TeleportTab:CreateSection("í”Œë ˆì´ì–´ TP")
TeleportTab:CreateButton({Name="ğŸ”„ ìƒˆë¡œê³ ì¹¨",Callback=function()if d then d:Set(gP())end Rayfield:Notify({Title="ìƒˆë¡œê³ ì¹¨",Content=#gP().."ëª…",Duration=1})end})
local d=TeleportTab:CreateDropdown({Name="í”Œë ˆì´ì–´ ì„ íƒ",Options=gP(),CurrentOption="",Flag="PD",Callback=function(o)selectedPlayer=o end})
TeleportTab:CreateButton({Name="âš¡ ì„ íƒí•œ í”Œë ˆì´ì–´ì—ê²Œ TP",Callback=function()
    if not selectedPlayer then Rayfield:Notify({Title="ì˜¤ë¥˜",Content="í”Œë ˆì´ì–´ ì„ íƒ í•„ìš”",Duration=1.5})return end
    local t=game.Players:FindFirstChild(selectedPlayer);if not t then Rayfield:Notify({Title="ì˜¤ë¥˜",Content="í”Œë ˆì´ì–´ ì—†ìŒ",Duration=1.5})return end
    local tc=t.Character;if not tc then Rayfield:Notify({Title="ì˜¤ë¥˜",Content="ìºë¦­í„° ì—†ìŒ",Duration=1.5})return end
    local tr=tc:FindFirstChild("HumanoidRootPart");if not tr then Rayfield:Notify({Title="ì˜¤ë¥˜",Content="ìœ„ì¹˜ ì—†ìŒ",Duration=1.5})return end
    wait(0.1);if tp(tr.Position+Vector3.new(0,3,0))then Rayfield:Notify({Title="ì„±ê³µ",Content=selectedPlayer.."ì—ê²Œ ì´ë™",Duration=1.5})end
end})

spawn(function()while wait(3)do if d then d:Set(gP())end end end)

-- ê·¸ ì™¸ íƒ­
OtherTab:CreateSection("ê¸°íƒ€")
OtherTab:CreateButton({Name="ì„œë²„ ë§í¬ ë³µì‚¬",Callback=function()setclipboard(string.format("https://www.roblox.com/games/%d?privateServerLinkCode=%s",game.PlaceId,game.JobId))Rayfield:Notify({Title="ë³µì‚¬ë¨",Content="ë§í¬ ë³µì‚¬ ì™„ë£Œ",Duration=1.5})end})
OtherTab:CreateButton({Name="ê²Œì„ ID ë³µì‚¬",Callback=function()setclipboard(tostring(game.GameId))Rayfield:Notify({Title="ë³µì‚¬ë¨",Content="ID: "..game.GameId,Duration=1.5})end})
OtherTab:CreateButton({Name="ìºë¦­í„° ë¦¬ìŠ¤í°",Callback=function()local h=game.Players.LocalPlayer.Character;if h then h=h:FindFirstChildOfClass("Humanoid")if h then h.Health=0 end end end})
OtherTab:CreateButton({Name="ëª¨ë“  ê¸°ëŠ¥ ë„ê¸°",Callback=function()
    fEnabled=false;setupFly();sEnabled=false;setupSpeed();jEnabled=false;setupJump();tEnabled=false;setupTP();gEnabled=false;workspace.Gravity=196.2;aimEnabled=false
    fT:Set(false);sT:Set(false);jT:Set(false);tT:Set(false);gT:Set(false);aT:Set(false)
    Rayfield:Notify({Title="ì´ˆê¸°í™”",Content="ëª¨ë“  ê¸°ëŠ¥ OFF",Duration=1.5})
end})

